

<main id="main" class="main ">

     

    <div class="row ">
        <div class="col-md-9">
            <h3>Order History</h3>
        </div>

       

    </div>

    <table id="brandTable" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>


                <th>Customer Name</th>
                <th>Product Name</th>
                <th>Price</th>
            



            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>


</main>

@section Scripts {
    <script>
        $(document).ready(function () {
            $('#brandTable').DataTable({
                "ajax": {
                    "url": "/Staff/OrderHistory/OrderHistory",
                    "type": "GET",
                    "datatype": "json"
                },
                "columns": [
                    { "data": "customerFullName" },
                    { "data": "productName" },
                    { "data": "price" },
                   



                ]
            });
        });
    </script>
}
